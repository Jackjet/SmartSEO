@model SmartSEO.Models.WorlkLog
@{
    ViewBag.Title = "添加工作日志";
}
<div class="span2">
    <div class="well sidebar-nav">
        @Html.Partial("_QuickMenu")
    </div>
</div>
<div class="span10">
    <ul class="breadcrumb">
        <li><a href="@Url.Content("~/")">网络营销</a><span class="divider">/</span></li>
        <li><a href="@Url.Content("~/WorlkLog/")">工作日志</a><span class="divider">/</span></li>
        <li class="active">添加日志</li>
    </ul>
    <fieldset>
        <legend>添加日志</legend>
        <form class="form-vertical" action="@Url.Content("~/WorlkLog/Add")" method="post">
        <div class="control-group">
            <label class="control-label">姓名</label>
            <div class="controls">
                <input type="text" name="UserName" class="span2" value="@Model.UserName">
            </div>
        </div>

        <div class="control-group">
            <label class="control-label">工作内容</label>
            <div class="controls">
                <textarea name="WorlkContent" class="span6" id="WorlkContent" rows="5">
                    @Model.WorlkContent
                </textarea>
            </div>
        </div>

        <div class="control-group">
            <label class="control-label">使用时间</label>
            <div class="controls">
                <input type="text" name="UseTime" class="span2" value="@Model.UseTime">
                单位/小时
            </div>
        </div>

        <div class="form-actions">
            <button type="submit" class="btn btn-primary">
                提交
            </button>
            <button type="button" class="btn">
                取消
            </button>
        </div>
        </form>
    </fieldset>
</div>
